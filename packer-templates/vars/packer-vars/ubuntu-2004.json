{
  "vm_id": "9000",
  "os": "l26",
  "cores": "2",
  "sockets": "1",
  "memory": "1024",
  "bridge": "vmbr0",
  "disk_type": "scsi",
  "disk_size": "10G",
  "disk_format": "raw",
  "disk_cache_mode": "writeback",
  "datastore": "local-lvm",
  "datastore_type": "directory",
  "cloud_init_storage_pool": "local-lvm",

  "iso": "local:iso/ubuntu-20.04.3-live-server-amd64.iso",
  "template_name": "ubuntu-server-20-04-packer-template",
  "template_description": "Ubuntu 20.04, generated by packer on {{ isotime \"2006-01-02T15:04:05Z\" }}",

  "ssh_password": "packer",
  "ssh_username": "packer",
  "ssh_timeout": "60m",

  "http_directory": "http",

  "boot_command": [
    "<esc><wait><esc><wait><f6><wait><esc><wait>",
    "<bs><bs><bs><bs><bs>",
    "autoinstall ds=nocloud-net;s=http://{{ .HTTPIP }}:{{ .HTTPPort }}/ ",
    "--- <enter>"
  ]
}
